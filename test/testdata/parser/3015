<script>
    $(function () {
        InitLeftBarMagicScroll();
    });

    function InitLeftBarMagicScroll() {
        if (window.smController != null) {
            window.smController.destroy(true);
        }
        // init controller
        window.smController = new ScrollMagic.Controller();


            // create a scene
            new ScrollMagic.Scene({
                triggerElement: $('.subgroup-scroll-top-545'),
                triggerHook: 0,
                duration: 1
            })
                .on("leave", function (event) {
                    if (event.scrollDirection == 'FORWARD') {
                        $('.subgroup-name').removeClass('active');
                        $('.subgroup-545').addClass("active");
                    }
                })
                .addTo(window.smController);

            // create a scene
            new ScrollMagic.Scene({
                triggerElement: $('.subgroup-scroll-end-545'),
                triggerHook: 0,
                duration: 1
            })
                .on("enter", function (event) {
                    if (event.scrollDirection == 'REVERSE') {
                        $('.subgroup-name').removeClass('active');
                        $('.subgroup-545').addClass("active");
                    }
                })
                .addTo(window.smController);

            // create a scene
            new ScrollMagic.Scene({
                triggerElement: $('.subgroup-scroll-top-321'),
                triggerHook: 0,
                duration: 1
            })
                .on("leave", function (event) {
                    if (event.scrollDirection == 'FORWARD') {
                        $('.subgroup-name').removeClass('active');
                        $('.subgroup-321').addClass("active");
                    }
                })
                .addTo(window.smController);

            // create a scene
            new ScrollMagic.Scene({
                triggerElement: $('.subgroup-scroll-end-321'),
                triggerHook: 0,
                duration: 1
            })
                .on("enter", function (event) {
                    if (event.scrollDirection == 'REVERSE') {
                        $('.subgroup-name').removeClass('active');
                        $('.subgroup-321').addClass("active");
                    }
                })
                .addTo(window.smController);
                }
</script>

