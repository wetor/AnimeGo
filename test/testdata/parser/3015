        var tool = {};
        (function () {

            var inputPEl = $('.m-tool-search-input');
            var inputEl = inputPEl.find('input');
            var changeEl = $('.m-tool-search-change');
            inputPEl.hide();
            tool.clickSearch = clickSearch;
            tool.resetSearch = resetSearch;

            function clickSearch() {
                changeEl.hide();
                inputPEl.show();
                inputEl.focus();
            }

            function resetSearch() {
                changeEl.show();
                inputPEl.hide();
                inputEl.val('');
            }
        })();
    </script>

