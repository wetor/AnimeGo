<script>
    $(function () {
        InitLeftBarMagicScroll();
    });

    function InitLeftBarMagicScroll() {
        if (window.smController != null) {
            window.smController.destroy(true);
        }
        // init controller
        window.smController = new ScrollMagic.Controller();


        // create a scene
        new ScrollMagic.Scene({
            triggerElement: $('.subgroup-scroll-top-615'),
            triggerHook: 0,
            duration: 1
        })
            .on("leave", function (event) {
                if (event.scrollDirection == 'FORWARD') {
                    $('.subgroup-name').removeClass('active');
                    $('.subgroup-615').addClass("active");
                }
            })
            .addTo(window.smController);

        // create a scene
        new ScrollMagic.Scene({
            triggerElement: $('.subgroup-scroll-end-615'),
            triggerHook: 0,
            duration: 1
        })
            .on("enter", function (event) {
                if (event.scrollDirection == 'REVERSE') {
                    $('.subgroup-name').removeClass('active');
                    $('.subgroup-615').addClass("active");
                }
            })
            .addTo(window.smController);

        // create a scene
        new ScrollMagic.Scene({
            triggerElement: $('.subgroup-scroll-top-1'),
            triggerHook: 0,
            duration: 1
        })
            .on("leave", function (event) {
                if (event.scrollDirection == 'FORWARD') {
                    $('.subgroup-name').removeClass('active');
                    $('.subgroup-1').addClass("active");
                }
            })
            .addTo(window.smController);

        // create a scene
        new ScrollMagic.Scene({
            triggerElement: $('.subgroup-scroll-end-1'),
            triggerHook: 0,
            duration: 1
        })
            .on("enter", function (event) {
                if (event.scrollDirection == 'REVERSE') {
                    $('.subgroup-name').removeClass('active');
                    $('.subgroup-1').addClass("active");
                }
            })
            .addTo(window.smController);

